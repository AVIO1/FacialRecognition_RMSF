<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>CFBundleDevelopmentRegion</key>
  <string>en</string> <!-- Replace with your app's development region (language) -->
  <key>CFBundleExecutable</key>
  <string>$(EXECUTABLE_NAME)</string> <!-- This is usually left as is -->
  <key>CFBundleIdentifier</key>
  <string>com.example.myapp</string> <!-- Replace with your app's bundle identifier -->
  <key>CFBundleInfoDictionaryVersion</key>
  <string>6.0</string> <!-- Keep it as it is -->
  <key>CFBundleName</key>
  <string>MyApp</string> <!-- Replace with your app's name -->
  <key>CFBundlePackageType</key>
  <string>APPL</string> <!-- For applications -->
  <key>CFBundleShortVersionString</key>
  <string>1.0</string> <!-- Replace with your app's version number -->
  <key>CFBundleSignature</key>
  <string>????</string> <!-- Not typically used -->
  <key>CFBundleVersion</key>
  <string>1</string> <!-- Replace with your app's build version number -->
</dict>
</plist>
